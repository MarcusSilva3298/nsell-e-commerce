meta {
  name: SignIn
  type: http
  seq: 1
}

post {
  url: {{baseUrl}}/auth/signIn
  body: json
  auth: none
}

body:json {
  {
    "email": "admin@email.com",
    "password": "123456"
  }
}

script:post-response {
  if (res.status !== 201) return
  bru.setEnvVar('accessToken', res.body.accessToken)
  bru.setEnvVar('refreshToken', res.body.refreshToken)
}
